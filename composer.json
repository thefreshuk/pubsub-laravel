{
  "name": "thefreshuk/pubsub-laravel",
  "description": "Provides PubSub support for Laravel apps",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "aws/aws-sdk-php-laravel": "^3.1",
    "illuminate/support": "^5.5",
    "illuminate/contracts": "^5.7"
  },
  "require-dev": {
    "phpunit/phpunit": ">=7.0"
  },
  "autoload": {
    "psr-4": {
      "TheFresh\\PubSub\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TheFresh\\PubSub\\": "test"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TheFresh\\PubSub\\PubSubServiceProvider"
      ]
    }
  }
}
