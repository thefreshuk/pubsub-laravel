{
  "name": "thefreshuk/pubsub-laravel",
  "description": "Provides PubSub support for Laravel apps",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "aws/aws-sdk-php-laravel": "^3.1",
    "aws/aws-php-sns-message-validator": "2.0.1",
    "guzzlehttp/guzzle": "^7.0.1",
    "symfony/console": "^6.2",
    "league/commonmark": "^2.2.1",
    "psr/container": "^1.1.1|^2.0.1",
    "psr/log": "^1.0|^2.0|^3.0",
    "psr/simple-cache": "^1.0|^2.0|^3.0",
    "ramsey/uuid": "^4.7",
    "symfony/error-handler": "^6.0",
    "symfony/finder": "^6.0",
    "symfony/http-foundation": "^6.0",
    "symfony/http-kernel": "^6.0",
    "symfony/mailer": "^6.0",
    "symfony/mime": "^6.0",
    "symfony/process": "^6.0",
    "symfony/routing": "^6.0",
    "symfony/uid": "^6.0",
    "symfony/var-dumper": "^6.0",
    "voku/portable-ascii": "^2.0",
    "illuminate/console": "^10.9",
    "illuminate/support": "^10.9",
    "illuminate/filesystem": "^10.9",
    "illuminate/routing": "^10.9",
    "illuminate/collections": "^10.9"
  },
  "require-dev": {
    "phpunit/phpunit": ">=7.0",
    "mockery/mockery": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "TheFresh\\PubSub\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TheFresh\\PubSub\\PubSubServiceProvider"
      ]
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:thefreshuk/aws-php-sns-message-validator.git"
    }
  ]
}
